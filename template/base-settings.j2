system {
    host-name {{ inventory_hostname }};
    login {
        user gns3 {
            uid 2000;
            class super-user;
            authentication {
                ssh-rsa "ssh-rsa  gns3@osboxes"
            }
        }
    }
    replace:
    name-server {
        {{ dns1 }};
        {{ dns2 }};
    }
    services {
        netconf {
            ssh;
        }
    }
}
